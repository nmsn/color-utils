{
  "name": "@nmsn/color-utils",
  "version": "0.2.0",
  "description": "A color transform utils collection.",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.mjs",
  "types": "./dist/types/index.d.ts",
