    "@rollup/plugin-typescript": "^8.5.0",
    "@types/color-name": "^1.1.1",
    "@types/jest": "^28.1.8",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "lint-staged": "^13.1.1",
    "release-toolbox": "0.1.0-2",
    "rollup": "^2.79.1",
    "ts-jest": "^28.0.8",
