    "@rollup/plugin-typescript": "^11.1.6",
    "@types/color-name": "^1.1.3",
    "@types/jest": "^29.5.12",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "lint-staged": "^14.0.1",
    "release-toolbox": "0.2.0",
    "rollup": "^3.29.4",
